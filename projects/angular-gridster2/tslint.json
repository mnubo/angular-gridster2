{
  "extends": ["../../tslint.json", "tslint-config-prettier"],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "gridster",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "gridster",
      "kebab-case"
    ],
    "no-inferrable-types": [true, "ignore-properties"]
  }
}
